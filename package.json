{
  "name": "template-vitejs-react18-typescript-eslint-prettier-husky",
  "private": true,
  "version": "0.0.2",
  "homepage": "https://github.com/javuscriptus/Template-ViteJS-React18-Typescript-Eslint-Prettier-Husky/blob/main/README.md",
  "url": "https://github.com/javuscriptus/Template-ViteJS-React18-Typescript-Eslint-Prettier-Husky/issues",
  "author": "JavuScriptus <javus.scriptus911@gmail.com> (https://github.com/javuscriptus)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix "
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "typescript": "^4.6.3",
    "url": "^0.11.0",
    "vite": "^2.9.9",
    "vite-plugin-fonts": "^0.5.0",
    "vite-plugin-svgr": "^2.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "yarn lint"
    ]
  }
}
