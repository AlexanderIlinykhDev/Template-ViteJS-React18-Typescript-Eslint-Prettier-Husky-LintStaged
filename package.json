{
  "name": "template-vitejs-react18-typescript-eslint-prettier-husky",
  "private": true,
  "version": "0.0.2",
  "homepage": "https://github.com/javuscriptus/Template-ViteJS-React18-Typescript-Eslint-Prettier-Husky/blob/main/README.md",
  "url": "https://github.com/javuscriptus/Template-ViteJS-React18-Typescript-Eslint-Prettier-Husky/issues",
  "author": "JavuScriptus <javus.scriptus911@gmail.com> (https://github.com/javuscriptus)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix "
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.5",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vitejs/plugin-react": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-simple-import-sort": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "typescript": "latest",
    "url": "latest",
    "vite": "latest",
    "vite-plugin-compression": "latest",
    "vite-plugin-eslint": "latest",
    "vite-plugin-fonts": "latest",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "latest"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "yarn lint"
    ]
  }
}
